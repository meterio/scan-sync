{
  "apps": [
    {
      "name": "scan-api-main",
      "instances": "1",
      "script": "./src/main.ts",
      "args": "api -n main -p 4000",
      "interpreter": "ts-node",
      "env": {
        "NODE_ENV": "production"
      }
    },
    {
      "name": "scan-api-main-standby",
      "instances": "1",
